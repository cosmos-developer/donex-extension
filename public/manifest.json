{
  "manifest_version": 3,
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "short_name": "React App",
  "name": "Create React App Sample",
  "permissions": ["scripting", "activeTab","management","storage"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["scripts/content-script.js"]
    }
  ]
}
