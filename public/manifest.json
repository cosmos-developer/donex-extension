{
  "manifest_version": 3,
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "short_name": "Donex",
  "name": "Donex",
  "permissions": ["scripting", "activeTab","management","storage"],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["scripts/content-script.js"]
    }
  ]
}
